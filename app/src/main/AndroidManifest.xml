<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.xq.progress.progressshadeproject"
    xmlns:tools="http://schemas.android.com/tools">
<!--  android:theme="@style/AppTheme"  ,android:theme-->
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="Android:icon,android:theme"
        android:name=".XqApplication">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".FillActivity"
            android:exported="false"
            android:screenOrientation="portrait"
            ></activity>

        <activity android:name=".SuspendMenuActivity"
            android:exported="false"
            android:screenOrientation="portrait"
           ></activity>

        <activity android:name=".BottomMiddleMenuActivity"
            android:exported="false"
            android:screenOrientation="portrait"
          ></activity>

        <activity android:name=".CoordinatorLayoutActivity"
            android:exported="false"
            android:screenOrientation="portrait"
            android:theme="@style/MyTheme"
           ></activity>

        <activity android:name=".pullRefleshActivity"
            android:exported="false"
            android:screenOrientation="portrait"
            android:theme="@style/MyTheme"
            ></activity>


    </application>

</manifest>